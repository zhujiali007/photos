page{
    
}
.container{
    width: 100%;
    height: 100%;
     display: block;
    overflow: hidden;
    text-align: center;
}
.tip{
    color: #ffffff;
    font-size: 18rpx;
}
.background {
width: 100%;
height: 100%;
display: block;
position: fixed;
z-index: -1;

}
.photo{
    width: 600rpx;
    height: 600rpx;
    background: #ffffff;
    padding: 20rpx;
    box-sizing: border-box;
    margin: auto;

   
}
.button{
    width: 100%;
    padding: 30rpx;
    box-sizing: border-box;
}
button{
    background: #4c4b7e;
    width: 284rpx;
    color: #ffffff;
    position: relative;
    display: block;
    margin-left: auto;
    margin-right: auto;
    margin-top: 50rpx;
    padding: 8px 24px;
    box-sizing: border-box;
    font-weight: 700;
    font-size: 17px;
    text-align: center;
    text-decoration: none;
    line-height: 1.41176471;
    border-radius: 4px;
    -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
    overflow: hidden;
}
.typeImg{
    width: 100%;
    display: flex;
    flex-direction: row;
    flex-wrap: wrap;
    justify-content: center;
    padding-top: 20rpx;
}
.typeImg .imgBox{
    width: 150rpx;
    height: 150rpx;
    position: relative;
}
.typeImg text{
    display: block;
    font-size: 18rpx;
    background: #000000;
    opacity: 0.6;
    color: #ffffff;
    position: absolute;
    bottom: 0;
    left: 0;
    width: 150rpx;
    text-align: center;
}
.active{
    background: #ffffff;
    opacity: 0.6;
    position: absolute;
    width:150rpx;
    height: 150rpx;
    top: 0;
    left: 0;
}